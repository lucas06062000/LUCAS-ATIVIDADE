package lucas06062000

public  class  Endereco {
    public  String cep;
    public  String logradouro;
     bairro público de String ;
    public  String cidade;
    public  String estado;
    public  String complemento;
    public  int numero;

    public  Endereco ( String  cep , String  logradouro , int  numero , String  complemento , String  bairro , String  cidade ,
            String  estado ) {

        isso . cep = cep;
        isso . logradouro = logradouro;
        isso . numero = numero;
        isso . complemento = complemento;
        isso . bairro = bairro;
        isso . cidade = cidade;
        isso . estado = estado;

    }

    public  String  getCep () {
        return cep;
    }

    public  String  getLogradouro () {
        voltar logradouro;
    }

    public  int  getNumero () {
        return numero;
    }

    public  String  getBizona () {
        voltar bairro;
    }

    public  String  getCidade () {
        voltar cidade;
    }

    public  String  getComplemento () {
        return complemento;
    }

    public  String  getEstado () {
        return estado;
    }

    @Sobrepor
    public  String  toString () {
        retornar  " \ n CEP: "  + cep +  " \ n Logradouro: "  + logradouro +  " \ n Nº: "  + numero +  " \ n Complemento: "  + complemento
                +  " \ n Bairro: "  + bairro +  " \ n Cidade: "  + cidade +  " \ n Estado: "  + estado +  " \ n \ n " ;
    }
}
